            <Button fx:id="deleteHangMucButton" layoutX="556.0" layoutY="301.0" mnemonicParsing="false" onAction="#DeleteHangMuc" prefHeight="29.0" prefWidth="230.0" text="DELETE HANG MUC" />
            <Button fx:id="nextScreenButton" layoutX="556.0" layoutY="349.0" mnemonicParsing="false" onAction="#NextScreen" prefHeight="29.0" prefWidth="230.0" text="NEXT" textFill="#110000" />
         </children>
      </AnchorPane>
      <Button fx:id="backButton" layoutX="17.0" layoutY="15.0" mnemonicParsing="false" onAction="#sceneSwitcher" text="&lt;" />
   </children>
</AnchorPane>
