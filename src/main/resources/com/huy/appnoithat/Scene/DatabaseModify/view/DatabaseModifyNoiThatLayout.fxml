<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="620.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.huy.appnoithat.Controller.DatabaseModify.DatabaseModifyNoiThatController">
   <children>
      <ListView fx:id="listViewNoiThat" layoutX="14.0" layoutY="78.0" onMouseClicked="#tableClickToSelectItem" prefHeight="501.0" prefWidth="639.0" />
      <Label layoutX="112.0" layoutY="21.0" prefHeight="26.0" prefWidth="288.0" text="Danh sách nội thất" textFill="#171616">
         <font>
            <Font name="DejaVu Serif Bold Italic" size="24.0" />
         </font>
      </Label>
      <Button fx:id="backButton" layoutX="14.0" layoutY="18.0" mnemonicParsing="false" onAction="#sceneSwitcher">
         <graphic>
            <ImageView fitHeight="21.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" styleClass="Button">
               <image>
                  <Image url="@../../icons/back.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="addButton" contentDisplay="RIGHT" layoutX="750.0" layoutY="482.0" mnemonicParsing="false" onAction="#addAction" prefHeight="29.0" prefWidth="357.0" styleClass="windows7-default" text="Thêm">
         <graphic>
            <ImageView fitHeight="21.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" styleClass="Button">
               <image>
                  <Image url="@../../icons/addPC.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="deleteButton" contentDisplay="RIGHT" layoutX="750.0" layoutY="521.0" mnemonicParsing="false" onAction="#deleteAction" prefHeight="29.0" prefWidth="357.0" styleClass="windows7-default" text="Xóa">
         <graphic>
            <ImageView fitHeight="21.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" styleClass="Button">
               <image>
                  <Image url="@../../icons/deletePC.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="nextButton" contentDisplay="RIGHT" layoutX="750.0" layoutY="560.0" mnemonicParsing="false" onAction="#nextAction" prefHeight="29.0" prefWidth="357.0" styleClass="windows7-default" text="Tiếp" textFill="#110000">
         <graphic>
            <ImageView fitHeight="21.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" styleClass="Button">
               <image>
                  <Image url="@../../icons/next-button.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <ListView fx:id="childrenList" layoutX="695.0" layoutY="78.0" onMouseClicked="#tableClickToSelectItem" prefHeight="378.0" prefWidth="474.0" />
   </children>
</AnchorPane>
