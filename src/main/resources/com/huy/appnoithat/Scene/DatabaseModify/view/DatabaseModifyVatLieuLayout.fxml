<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="620.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/20.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.huy.appnoithat.Controller.DatabaseModify.DatabaseModifyVatLieuController">
    <children>
        <AnchorPane layoutX="10.0" prefHeight="530.0" prefWidth="900.0" AnchorPane.bottomAnchor="0.0"
                    AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <ListView fx:id="listViewVatLieu" layoutX="14.0" layoutY="67.0" onMouseClicked="#tableClickToSelectItem"
                          prefHeight="501.0" prefWidth="639.0"/>
                <Label layoutX="103.0" layoutY="16.0" prefHeight="26.0" prefWidth="288.0" text="Danh sách vật liệu"
                       textFill="#171616">
                    <font>
                        <Font name="DejaVu Serif Bold" size="24.0"/>
                    </font>
                </Label>
                <Button fx:id="backButton" layoutX="13.0" layoutY="17.0" mnemonicParsing="false"
                        onAction="#sceneSwitcher">
                    <graphic>
                        <ImageView fitHeight="21.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true"
                                   styleClass="Button">
                            <image>
                                <Image url="@../../icons/back.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <TableView fx:id="tableViewThongSo" layoutX="686.0" layoutY="67.0" prefHeight="255.0" prefWidth="480.0">
                    <columns>
                        <TableColumn fx:id="Dai" prefWidth="75.0" text="Dài"/>
                        <TableColumn fx:id="Rong" prefWidth="75.0" text="Rộng"/>
                        <TableColumn fx:id="Cao" prefWidth="75.0" text="Cao"/>
                        <TableColumn fx:id="DonVi" prefWidth="99.0" text="Đơn vị tính"/>
                        <TableColumn fx:id="DonGia" prefWidth="99.0" text="Đơn giá"/>
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                </TableView>
            </children>
        </AnchorPane>
        <Label layoutX="432.0" layoutY="-102.0" text="Vat lieu">
            <font>
                <Font name="Dyuthi Regular" size="18.0"/>
            </font>
        </Label>
        <Button fx:id="addButton" contentDisplay="RIGHT" layoutX="754.0" layoutY="350.0" mnemonicParsing="false"
                onAction="#addAction" prefHeight="29.0" prefWidth="357.0" styleClass="windows7-default" text="Thêm">
            <graphic>
                <ImageView fitHeight="21.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true"
                           styleClass="Button">
                    <image>
                        <Image url="@../../icons/addPC.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="deleteButton" contentDisplay="RIGHT" layoutX="754.0" layoutY="389.0" mnemonicParsing="false"
                onAction="#deleteAction" prefHeight="29.0" prefWidth="357.0" styleClass="windows7-default" text="Xóa">
            <graphic>
                <ImageView fitHeight="21.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true"
                           styleClass="Button">
                    <image>
                        <Image url="@../../icons/deletePC.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="nextButton" contentDisplay="RIGHT" layoutX="754.0" layoutY="428.0" mnemonicParsing="false"
                onAction="#nextAction" prefHeight="29.0" prefWidth="357.0" styleClass="windows7-default" text="Tiếp"
                textFill="#110000">
            <graphic>
                <ImageView fitHeight="21.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true"
                           styleClass="Button">
                    <image>
                        <Image url="@../../icons/next-button.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </children>
</AnchorPane>
